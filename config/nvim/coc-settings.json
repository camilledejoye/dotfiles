{
    "phpactor.enable": true,
    "phpactor.config": {
        "language_server_completion.trim_leading_dollar": true
    },
    "languageserver": {
        "phpactor_debug": {
            "host": "0.0.0.0",
            "port": 8088,
            "filetypes": ["php", "cucumber"],
            "initializationOptions": {
                "language_server_completion.trim_leading_dollar": true
            },
            "settings": {
            },
            "enable": false
        }
    },
    "suggest.defaultSortMethod": "alphabetical",
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "signature.maxWindowWidth": 120,
    "signature.preferShownAbove": true,
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "diagnostic.displayByAle": false,
    "cursors.cancelKey": "<Esc>",
    "cursors.nextKey": "<C-j>",
    "cursors.previousKey": "<C-k>"
}
